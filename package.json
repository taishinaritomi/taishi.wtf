{
  "name": "@taishi-wtf/monorepo",
  "version": "1.0.0",
  "author": "Taishi Naritomi",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:turbo": "turbo dev",
    "dev:cargo": "cargo watch -x run",
    "build": "run-p build:*",
    "build:turbo": "turbo build",
    "build:cargo": "cargo build",
    "preview": "run-p preview:*",
    "preview:turbo": "turbo preview",
    "preview:cargo": "cargo run",
    "fix": "run-p fix:*",
    "fix:biome": "biome check --write .",
    "fix:terraform": "terraform fmt",
    "fix:cargo": "cargo fix --allow-no-vcs",
    "fix:cargo-clippy": "cargo clippy --fix --allow-no-vcs",
    "fix:cargo-fmt": "cargo fmt",
    "check": "run-p check:*",
    "check:turbo": "turbo check",
    "check:biome": "biome check .",
    "check:cargo": "cargo check",
    "check:cargo-clippy": "cargo clippy",
    "check:cargo-fmt": "cargo fmt --check"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.10",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.16.2",
    "turbo": "^2.0.9",
    "typescript": "^5.5.3",
    "wrangler": "^3.64.0"
  }
}
