{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "preserve",
    "noEmit": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "preserveWatchOutput": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "checkJs": false,
    "types": ["@cloudflare/workers-types"]
  }
}
