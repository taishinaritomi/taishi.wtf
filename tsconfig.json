    "target": "esnext",
    "module": "nodenext",
    "noEmit": true,
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "preserveWatchOutput": true,
    "jsx": "preserve",
    "checkJs": false,
    "types": ["@cloudflare/workers-types"]
  }
}
